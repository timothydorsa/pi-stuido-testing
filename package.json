{
  "name": "pi-studio-testing",
  "version": "1.0.0",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "start": "concurrently \"npm run react:dev\" \"npm run electron:dev\"",
    "react:dev": "webpack serve --config webpack.config.js",
    "react:build": "webpack --mode production",
    "electron:dev": "wait-on http://localhost:3002 && NODE_ENV=development electron .",
    "electron:pack": "electron-builder",
    "build": "npm run react:build && npm run electron:pack",
    "dev": "npm start",
    "backend:dev": "nodemon backend/server.js",
    "backend": "node backend/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "electron",
    "react",
    "system-monitor",
    "ipc",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "description": "React Electron Node Express application with IPC communication, system monitoring, and real-time preview functionality",
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@tanstack/react-query": "^5.85.0",
    "axios": "^1.11.0",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "cpu-stat": "^2.0.1",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "express": "^5.1.0",
    "highlight.js": "^11.11.1",
    "html-webpack-plugin": "^5.6.3",
    "nodemon": "^3.1.10",
    "pidusage": "^4.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-json-pretty": "^2.2.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-tabs": "^6.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "systeminformation": "^5.27.7",
    "uuid": "^11.1.0",
    "wait-on": "^8.0.4",
    "webpack": "^5.101.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "ws": "^8.18.3"
  },
  "build": {
    "appId": "com.example.pi-studio-testing",
    "productName": "System Monitor Dashboard",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "backend/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^7.1.2",
    "global": "^4.4.0",
    "https-browserify": "^1.0.0",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "node-ssh": "^13.2.1",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "sass": "^1.90.0",
    "sass-loader": "^16.0.5",
    "ssh2": "^1.16.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^4.0.0",
    "url": "^0.11.4",
    "util": "^0.12.5"
  }
}
