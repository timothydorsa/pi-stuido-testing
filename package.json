{
  "name": "pi-studio-testing",
  "version": "1.0.0",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "start": "concurrently \"npm run react:dev\" \"npm run backend:dev\" \"npm run electron:dev\"",
    "start:frontend": "concurrently \"npm run react:dev\" \"npm run electron:dev\"",
    "start:full": "concurrently \"npm run react:dev\" \"npm run backend:dev\" \"npm run electron:dev\"",
    "start:web": "concurrently \"npm run react:dev\" \"npm run backend:dev\"",
    "start:clean": "npm run stop && npm run start",
    "stop": "pkill -f 'node.*server.js' || pkill -f 'webpack' || pkill -f 'electron' || true",
    "restart": "npm run stop && sleep 2 && npm run start",
    "health": "curl -s http://localhost:8001/health | jq .",
    "status": "node scripts/status.js",
    "logs:backend": "tail -f backend/logs/server.log 2>/dev/null || echo 'No backend logs found'",
    "test:api": "curl -s http://localhost:8001/api/processes | jq '.processes | length'",
    "test:ws": "node -e \"const ws=require('ws').WebSocket;const c=new ws('ws://localhost:8001');c.on('message',d=>{const m=JSON.parse(d);if(m.type==='metrics')console.log('âœ… WebSocket working, processes:',m.processes?.top?.length||0);});setTimeout(()=>process.exit(0),3000);\"",
    "test:network-scan": "curl -s -X POST http://localhost:8001/api/network-scan -H 'Content-Type: application/json' -d '{\"subnet\": \"192.168.1.0/28\", \"enableHostname\": true, \"enableManufacturer\": true}' | jq '.scanId'",
    "cancel:scans": "curl -s http://localhost:8001/api/scans/active | jq -r '.scans | keys[]' | xargs -I {} curl -s -X POST http://localhost:8001/api/network-scan/{}/cancel",
    "test:oui-lookup": "curl -s -X POST http://localhost:8001/api/oui/lookup -H 'Content-Type: application/json' -d '{\"mac\": \"00:1B:63:84:45:E6\"}' | jq '.result.manufacturerInfo'",
    "test:enhanced-scan": "curl -s -X POST http://localhost:8001/api/network-scan/enhanced -H 'Content-Type: application/json' -d '{\"subnet\": \"192.168.1.0/28\", \"enableManufacturer\": true, \"enableDeviceIntelligence\": true}' | jq .",
    "react:dev": "webpack serve --config webpack.config.js",
    "react:build": "webpack --mode production",
    "electron:dev": "wait-on http://localhost:3002 && NODE_ENV=development electron .",
    "electron:pack": "electron-builder",
    "build": "npm run react:build && npm run electron:pack",
    "dev": "npm start",
    "backend:dev": "nodemon backend/server.js",
    "backend": "node backend/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "licenses": "npm ls --json > licenses.json && node scripts/generate-attributions.js",
    "license-check": "npx license-checker --summary"
  },
  "keywords": [
    "electron",
    "react",
    "system-monitor",
    "ipc",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "description": "React Electron Node Express application with IPC communication, system monitoring, and real-time preview functionality",
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@tanstack/react-query": "^5.85.0",
    "arp-table": "^0.0.0",
    "axios": "^1.11.0",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "cpu-stat": "^2.0.1",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "express": "^5.1.0",
    "highlight.js": "^11.11.1",
    "html-webpack-plugin": "^5.6.3",
    "node-fetch": "^3.3.2",
    "node-nmap": "^4.0.0",
    "nodemon": "^3.1.10",
    "oui": "^13.0.5",
    "pidusage": "^4.0.1",
    "ping": "^0.4.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-json-pretty": "^2.2.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-tabs": "^6.1.0",
    "recharts": "^3.1.2",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "systeminformation": "^5.27.7",
    "uuid": "^11.1.0",
    "wait-on": "^8.0.4",
    "webpack": "^5.101.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "ws": "^8.18.3"
  },
  "build": {
    "appId": "com.example.pi-studio-testing",
    "productName": "System Monitor Dashboard",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "backend/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^7.1.2",
    "global": "^4.4.0",
    "https-browserify": "^1.0.0",
    "license-checker": "^25.0.1",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "node-ssh": "^13.2.1",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "sass": "^1.90.0",
    "sass-loader": "^16.0.5",
    "ssh2": "^1.16.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^4.0.0",
    "url": "^0.11.4",
    "util": "^0.12.5"
  }
}
